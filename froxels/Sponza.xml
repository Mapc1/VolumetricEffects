<?xml version="1.0" encoding="UTF-8"?>
<project name="Volumetric Effects - Froxels" width="1280" height="720">
    <assets>
        <attributes>
            <attribute type="STATE" name="ANISOTROPY" data="FLOAT" value="0.7"/>
            <attribute type="STATE" name="SCATTERING" data="VEC4" x="0.9" y="0.9" z="0.9" w="1.0"/>
            <attribute type="STATE" name="ABSORPTION" data="FLOAT" value="0.1"/>
            <attribute type="STATE" name="DENSITY" data="FLOAT" value="0.001"/>
            <attribute type="STATE" name="HOMOGENEOUS_MEDIA" data="INT" value="0"/>
            <attribute type="LIGHT" name="LIGHT_INTENSITY" data="FLOAT" value="50"/>
            <attribute type="LIGHT" name="AMBIENT_LIGHT_STRENGTH" data="FLOAT" value="0.0"/>
            <attribute type="RENDERER" name="VOL_ACTIVE" data="INT" value="0"/>
            <attribute type="CAMERA" name="PREV_PROJ_VIEW_MAT" data="MAT4"/>
        </attributes>
        
        <scenes>
            <scene name="Sponza" filename="..\models\sponza\sponza.obj"/>
            <scene name="SkyBox">
                <geometry name="Box" type="BOX" material="skyBox">
                    <SCALE x="4000" y="4000" z="4000"/>
                </geometry>
            </scene>
        </scenes>

        <viewports>
            <viewport name="MainViewport">
                <CLEAR_COLOR r="0.0" g="0.0" b="0.0"/>
            </viewport>
            <viewport name="CM_Viewport">
                <CLEAR_COLOR r="0.0" g="0.0" b="0.0"/>
                <SIZE width="1024" height="1024"/>
            </viewport>
        </viewports>

        <cameras>
            <camera name="MainCamera">
                <viewport name="MainViewport"/>
                <projection TYPE="PERSPECTIVE" FOV="60.0" NEAR="1.0" FAR="7000.0"/>
                <POSITION x="883.9" y="553.7" z="-42.4"/>
                <LOOK_AT_POINT x="882.9" y="553.5" z="-42.4"/>
                <UP x="0.0" y="1.0" z="0.0"/>
            </camera>

            <!-- Cubemap camera -->
            <camera name="CM_FOV_90">
                <viewport name="CM_Viewport" />
                <POSITION x="0" y="0" z="0.0" />
                <VIEW x="1.0" y="0.0" z="0.0" />
                <NEAR value = "1.0" />
                <FAR value="7000.0"/>
                <FOV value = "90" />
                <UP x="0" y="-1" z="0" />
                </camera>
        </cameras>

        <lights>
            <light name="Sun">
                <DIRECTION x="0.5" y="-0.7" z="0.4"/>
                <COLOR r="1.0" g="1.0" b="1.0"/>
            </light>

            <!-- Point lights -->
            <light name="PointLight1">
