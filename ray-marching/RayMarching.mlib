                             context   = "PointLight1"
                             component = "POSITION"
                    />

                    <!-- Cubemap camera props -->
                    <valueof uniform   = "CM_P"
                             type      = "CAMERA"
                             context   = "CM_FOV_90"
                             component = "PROJECTION_MATRIX"
                    />
                    <valueof uniform   = "CM_FAR"
                             type      = "CAMERA"
                             context   = "CM_FOV_90"
                             component = "FAR"
                    />

                    <!-- Renderer props -->
                    <valueof uniform   = "M"
                             type      = "RENDERER"
                             context   = "CURRENT"
                             component = "MODEL"
                    />
                </values>
            </shader>
            <state name="shadow"/>
