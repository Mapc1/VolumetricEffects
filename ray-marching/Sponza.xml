<?xml version="1.0" encoding="UTF-8"?>
<project name="Volumetric Effects - Ray-marching" width="1280" height="720">
    <assets>
        <attributes>
            <attribute type="STATE" name="NUM_STEPS" data="INT" value="64"/>
            <attribute type="STATE" name="ANISOTROPY" data="FLOAT" value="0.7"/>
            <attribute type="STATE" name="SCATTERING" data="VEC4" x="0.9" y="0.9" z="0.9" w="1.0"/>
            <attribute type="STATE" name="ABSORPTION" data="FLOAT" value="0.1"/>
            <attribute type="STATE" name="DENSITY" data="FLOAT" value="0.001"/>
            <attribute type="STATE" name="SKYBOX_FAR" data="INT" value="3500"/>
            <attribute type="RENDERER" name="DEBUG" data="INT" value="1"/>

            <attribute type="LIGHT" name="LIGHT_INTENSITY" data="FLOAT" value="5"/>

            <attribute type="RENDERER" name="AMBIENT_LIGHT_STRENGTH" data="FLOAT" value="0.0"/>
            <attribute type="RENDERER" name="VOL_ACTIVE" data="INT" value="0"/>
            <attribute type="RENDERER" name="GAMMA" data="FLOAT" value="2.2"/>
        </attributes>

        <scenes>
            <scene name="Sponza" filename="..\models\sponza\sponza.obj"/>
            <scene name="SkyBox">
                <geometry name="Box" type="BOX" material="skybox">
                    <SCALE x="-4000" y="-4000" z="-4000"/>
                </geometry>
            </scene>
            <scene name="LightSphere">
                <geometry name="LightSphere" type="SPHERE" SLICES="128" STACKS="128" material="LightSphere"/>
            </scene>
        </scenes>

        <viewports>
            <viewport name="MainViewport">
                <CLEAR_COLOR r="0.0" g="0.0" b="0.0"/>
            </viewport>
            <viewport name="CM_Viewport">
                <CLEAR_COLOR r="0.0" g="0.0" b="0.0"/>
                <SIZE width="1024" height="1024"/>
            </viewport>
        </viewports>

        <cameras>
            <camera name="MainCamera">
                <viewport name="MainViewport"/>
                <projection TYPE="PERSPECTIVE" FOV="60.0" NEAR="1.0" FAR="7000.0"/>
                <POSITION x="900" y="500" z="500"/>
                <LOOK_AT_POINT x="600" y="800" z="-400"/>
                <UP x="0.0" y="1.0" z="0.0"/>
            </camera>

            <!-- Cubemap camera -->
            <camera name="CM_FOV_90">
                <viewport name="CM_Viewport" />
                <POSITION x="0" y="0" z="0.0" />
                <VIEW x="1.0" y="0.0" z="0.0" />
                <NEAR value = "1.0" />
                <FAR value="7000.0"/>
                <FOV value = "90" />
                <UP x="0" y="-1" z="0" />
                </camera>
            </cameras>

        <lights>
            <light name="Sun">
                <DIRECTION x="0.5" y="-0.7" z="0.4"/>
                <COLOR r="1.0" g="1.0" b="1.0"/>
            </light>

            <!-- Point lights -->
            <light name="PointLight1">
                <COLOR r="0.6" g="0.1" b="0.1"/>
                <POSITION x="0.0" y="500.0" z="0.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0019"/>
            </light>
            <light name="PointLight2">
                <COLOR r="0.2" g="0.4" b="0.55"/>
                <POSITION x="0.0" y="700.0" z="500.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0019"/>
            </light>
            <light name="PointLight3">
                <COLOR r="0.2" g="0.4" b="0.55"/>
                <POSITION x="0.0" y="-700.0" z="-500.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0019"/>
            </light>
            <light name="PointLight4">
                <COLOR r="0.2" g="0.7" b="0.3"/>
                <POSITION x="1000.0" y="300.0" z="0.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0019"/>
            </light>
            <light name="PointLight5">
                <COLOR r="0.3" g="0.4" b="0.2"/>
                <POSITION x="-1000.0" y="200.0" z="300.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0019"/>
            </light>
            <light name="PointLight6">
                <COLOR r="0.7" g="0.5" b="0.5"/>
                <POSITION x="400.0" y="1000.0" z="0.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0019"/>
            </light>
            <light name="PointLight7">
                <COLOR r="0.2" g="0.8" b="0.8"/>
                <POSITION x="0.0" y="100.0" z="0.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0019"/>
            </light>
            <light name="PointLight8">
                <COLOR r="0.7" g="0.7" b="0.2"/>
                <POSITION x="300.0" y="200.0" z="-400.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0019"/>
            </light>
            <light name="PointLight9">
                <COLOR r="0.5" g="0.2" b="0.55"/>
                <POSITION x="0.0" y="0.0" z="500.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0019"/>
            </light>
            <light name="PointLight10">
                <COLOR r="0.5" g="0.8" b="0.55"/>
                <POSITION x="-1000.0" y="560.0" z="-100.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0019"/>
            </light>
        </lights>

        <materialLibs>
            <materialLib filename="RayMarching.mlib"/>
        </materialLibs>
    </assets>

    <pipelines>
        <pipeline name="ray-marching" defaultCamera="MainCamera">

            <pass class="depthmap" name="genShadowMap">
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <camera name="MainCamera"/>
                <lights>
                    <light name="Sun"/>
                </lights>
                <renderTarget name="shadowMap" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="shadowMap"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap1">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap1" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="pointLightShadowMap1"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap2">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap2" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="pointLightShadowMap2"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap3">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap3" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="pointLightShadowMap3"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap4">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap4" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="pointLightShadowMap4"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap5">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap5" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="pointLightShadowMap5"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap6">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap6" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="pointLightShadowMap6"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap7">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap7" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="pointLightShadowMap7"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap8">
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap8" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="pointLightShadowMap8"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap9">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap9" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="pointLightShadowMap9"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap10">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap10" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="pointLightShadowMap10"/>
                </materialMaps>
            </pass>

            <pass class="default" name="gBuffer">
                <preScript file="scripts/light_loading.lua" script="lights"/>
                <viewport name="MainViewport"/>
                <scenes>
                    <scene name="Sponza"/>
                    <scene name="SkyBox"/>
                </scenes>
                <camera name="MainCamera"/>
                <injectionMaps>
                    <map toMaterial="*">
                        <shader fromMaterial="gBuffer" fromLibrary="RayMarching" />
                    </map>
                </injectionMaps>
                <materialMaps>
                    <map fromMaterial="skybox" toLibrary="RayMarching" toMaterial="skybox_gBuffer"/>
                </materialMaps>
                <renderTarget name="gBuffer" fromLibrary="RayMarching"/>
            </pass>

            <pass class="quad" name="scatIntegration">
                <COLOR_CLEAR value="false" />
                <camera name="MainCamera"/>
                <viewport name="MainViewport"/>
                <material name="scatIntegration" fromLibrary="RayMarching"/>
                <renderTarget name="scatTrans" fromLibrary="RayMarching"/>
            </pass>

            <pass class="quad" name="gaussianBlurHorizontal">
                <COLOR_CLEAR value="false" />
                <viewport name="MainViewport"/>
                <material name="gaussianBlurHorizontal" fromLibrary="RayMarching"/>
                <renderTarget name="step1BlurScatTrans" fromLibrary="RayMarching"/>
            </pass>

            <pass class="quad" name="gaussianBlurVertical">
                <COLOR_CLEAR value="false" />
                <viewport name="MainViewport"/>
                <material name="gaussianBlurVertical" fromLibrary="RayMarching"/>
                <renderTarget name="finalBlurScatTrans" fromLibrary="RayMarching"/>
            </pass>

            <pass class="default" name="PointLightPass">
                <scenes instances="10">
                    <scene name="LightSphere"/>
                </scenes>
                <COLOR_CLEAR value="true"/>
                <viewport name="MainViewport"/>
                <camera name="MainCamera"/>
                <renderTarget name="pointLightIlumination" fromLibrary="RayMarching"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="RayMarching" toMaterial="pointLightIlumination"/>
                </materialMaps>
            </pass>

            <pass class="quad" name="render">
                <COLOR_CLEAR value="false" />
                <viewport name="MainViewport"/>
                <camera name="MainCamera"/>
                <material name="render" fromLibrary="RayMarching"/>
            </pass>
        </pipeline>


        <!-- Debug pipelines -->
        <pipeline name="Normals">
            <pass class="default" name="gBuffer">
                <viewport name="MainViewport"/>
                <scenes>
                    <scene name="Sponza"/>
                    <scene name="SkyBox"/>
                </scenes>
                <camera name="MainCamera"/>
                <injectionMaps>
                    <map toMaterial="*">
                        <shader fromMaterial="gBuffer" fromLibrary="RayMarching" />
                    </map>
                </injectionMaps>
                <materialMaps>
                    <map fromMaterial="skybox" toLibrary="RayMarching" toMaterial="skybox_gBuffer"/>
                </materialMaps>
                <renderTarget name="gBuffer" fromLibrary="RayMarching"/>
            </pass>

            <pass class="quad" name="normals">
                <COLOR_CLEAR value="false" />
                <viewport name="MainViewport"/>
                <material name="normals" fromLibrary="RayMarching"/>
            </pass>
        </pipeline>

        <pipeline name="Positions">
            <pass class="default" name="gBuffer">
                <viewport name="MainViewport"/>
                <scenes>
                    <scene name="Sponza"/>
