                             context   = "CURRENT"
                             component = "PROJECTION_VIEW_MODEL"
                    />
                </values>
            </shader>
            <state name="shadow"/>
        </material>

        <material name="pointLightShadowMap1">
            <shader name="pointLightShadowMap">
