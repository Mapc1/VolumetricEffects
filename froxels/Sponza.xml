                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0001"/>
            </light>
            <light name="PointLight5">
                <COLOR r="0.3" g="0.4" b="0.2"/>
                <POSITION x="-600.0" y="100.0" z="-300.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0001"/>
            </light>
            <light name="PointLight6">
                <COLOR r="0.7" g="0.5" b="0.5"/>
                <POSITION x="400.0" y="150.0" z="-100.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0001"/>
            </light>
            <light name="PointLight7">
                <COLOR r="0.2" g="0.8" b="0.8"/>
                <POSITION x="0.0" y="100.0" z="0.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0001"/>
            </light>
            <light name="PointLight8">
                <COLOR r="0.7" g="0.7" b="0.2"/>
                <POSITION x="300.0" y="200.0" z="-400.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0001"/>
            </light>
            <light name="PointLight9">
                <COLOR r="0.5" g="0.2" b="0.55"/>
                <POSITION x="270" y="250" z="350.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0001"/>
            </light>
            <light name="PointLight10">
                <COLOR r="0.5" g="0.8" b="0.55"/>
                <POSITION x="-1000.0" y="560.0" z="-100.0"/>
                <CONSTANT_ATT value="1"/>
                <LINEAR_ATT value="0.022"/>
                <QUADRATIC_ATT value="0.0001"/>
            </light>
        </lights>

        <materialLibs>
            <materialLib filename="Froxels.mlib"/>
        </materialLibs>
    </assets>

    <pipelines>
        <pipeline name="volfx" defaultCamera="MainCamera">
            <pass class="depthmap" name="genShadowMap">
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <camera name="MainCamera"/>
                <lights>
                    <light name="Sun"/>
                </lights>
                <renderTarget name="shadowMap" fromLibrary="Froxels"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="Froxels" toMaterial="shadowMap"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap1">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap1" fromLibrary="Froxels"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="Froxels" toMaterial="pointLightShadowMap1"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap2">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap2" fromLibrary="Froxels"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="Froxels" toMaterial="pointLightShadowMap2"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap3">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap3" fromLibrary="Froxels"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="Froxels" toMaterial="pointLightShadowMap3"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap4">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap4" fromLibrary="Froxels"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="Froxels" toMaterial="pointLightShadowMap4"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap5">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap5" fromLibrary="Froxels"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="Froxels" toMaterial="pointLightShadowMap5"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap6">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap6" fromLibrary="Froxels"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="Froxels" toMaterial="pointLightShadowMap6"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap7">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap7" fromLibrary="Froxels"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="Froxels" toMaterial="pointLightShadowMap7"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap8">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap8" fromLibrary="Froxels"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="Froxels" toMaterial="pointLightShadowMap8"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap9">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap9" fromLibrary="Froxels"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="Froxels" toMaterial="pointLightShadowMap9"/>
                </materialMaps>
            </pass>

            <pass class="default" name="genPointLightShadowMap10">
                <mode value="RUN_ONCE"/>
                <scenes>
                    <scene name="Sponza"/>
                </scenes>
                <renderTarget name="pointLightShadowMap10" fromLibrary="Froxels"/>
                <materialMaps>
                    <map fromMaterial="*" toLibrary="Froxels" toMaterial="pointLightShadowMap10"/>
                </materialMaps>
            </pass>

            <pass class="compute" name="GenDensityNoise">
                <mode value="RUN_ONCE"/>
                <material name="genDensityNoise" fromLibrary="Froxels" dimX="256" dimY="256" dimZ="256"/>
            </pass>

            <pass class="compute" name="CalcScattering">
                <preScript file="scripts/light_loading.lua" script="lights"/>
                <material name="calcScattering" fromLibrary="Froxels" dimX="160" dimY="90" dimZ="64"/>
            </pass>

            <pass class="compute" name="ScatIntegration">
                <material name="scatIntegration" fromLibrary="Froxels" dimX="160" dimY="90" dimZ="1"/>
            </pass>

            <pass class="default" name="Render">
                <scenes>
                    <scene name="Sponza"/>
                    <scene name="SkyBox"/>
                </scenes>
                <camera name="MainCamera"/>
                <lights>
                    <light name="Sun"/>
                </lights>
                <postScript file="scripts/temporal_matrix_set.lua" script="tms"/>
                <injectionMaps>
                    <map toMaterial="*">
                        <shader fromMaterial="render" fromLibrary="Froxels" />
                        <buffers>
                            <buffer name="positions" fromLibrary="Froxels">
                                <TYPE value="SHADER_STORAGE"/>
                                <BINDING_POINT value="1"/>
                            </buffer>
                            <buffer name="colors" fromLibrary="Froxels">
                                <TYPE value="SHADER_STORAGE"/>
                                <BINDING_POINT value="2"/>
                            </buffer>
                            <buffer name="intensities" fromLibrary="Froxels">
                                <TYPE value="SHADER_STORAGE"/>
                                <BINDING_POINT value="3"/>
                            </buffer>
                            <buffer name="constAtt" fromLibrary="Froxels">
                                <TYPE value="SHADER_STORAGE"/>
                                <BINDING_POINT value="4"/>
                            </buffer>
                            <buffer name="linearAtt" fromLibrary="Froxels">
                                <TYPE value="SHADER_STORAGE"/>
                                <BINDING_POINT value="5"/>
                            </buffer>
                            <buffer name="quadAtt" fromLibrary="Froxels">
                                <TYPE value="SHADER_STORAGE"/>
                                <BINDING_POINT value="6"/>
                            </buffer>
                            <buffer name="enableds" fromLibrary="Froxels">
                                <TYPE value="SHADER_STORAGE"/>
                                <BINDING_POINT value="5"/>
                            </buffer>
                        </buffers>
                        <textures>
                            <texture name="integrationTex" fromLibrary="Froxels" toUnit="2">
                                <WRAP_S value="CLAMP_TO_EDGE"/>
                                <WRAP_R value="CLAMP_TO_EDGE"/>
                                <WRAP_T value="CLAMP_TO_EDGE"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                            <texture name="shadowMap" fromLibrary="Froxels" toUnit="1">
                                <COMPARE_MODE value="COMPARE_REF_TO_TEXTURE"/>
                                <COMPARE_FUNC value="LEQUAL"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                            <texture name="pointLightShadowMap1" fromLibrary="Froxels" toUnit="3">
                                <COMPARE_MODE value="COMPARE_REF_TO_TEXTURE"/>
                                <COMPARE_FUNC value="LEQUAL"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                            <texture name="pointLightShadowMap2" fromLibrary="Froxels" toUnit="4">
                                <COMPARE_MODE value="COMPARE_REF_TO_TEXTURE"/>
                                <COMPARE_FUNC value="LEQUAL"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                            <texture name="pointLightShadowMap3" fromLibrary="Froxels" toUnit="5">
                                <COMPARE_MODE value="COMPARE_REF_TO_TEXTURE"/>
                                <COMPARE_FUNC value="LEQUAL"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                            <texture name="pointLightShadowMap4" fromLibrary="Froxels" toUnit="6">
                                <COMPARE_MODE value="COMPARE_REF_TO_TEXTURE"/>
                                <COMPARE_FUNC value="LEQUAL"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                            <texture name="pointLightShadowMap5" fromLibrary="Froxels" toUnit="7">
                                <COMPARE_MODE value="COMPARE_REF_TO_TEXTURE"/>
                                <COMPARE_FUNC value="LEQUAL"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                            <texture name="pointLightShadowMap6" fromLibrary="Froxels" toUnit="8">
                                <COMPARE_MODE value="COMPARE_REF_TO_TEXTURE"/>
                                <COMPARE_FUNC value="LEQUAL"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                            <texture name="pointLightShadowMap7" fromLibrary="Froxels" toUnit="9">
                                <COMPARE_MODE value="COMPARE_REF_TO_TEXTURE"/>
                                <COMPARE_FUNC value="LEQUAL"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                            <texture name="pointLightShadowMap8" fromLibrary="Froxels" toUnit="10">
                                <COMPARE_MODE value="COMPARE_REF_TO_TEXTURE"/>
                                <COMPARE_FUNC value="LEQUAL"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                            <texture name="pointLightShadowMap9" fromLibrary="Froxels" toUnit="11">
                                <COMPARE_MODE value="COMPARE_REF_TO_TEXTURE"/>
                                <COMPARE_FUNC value="LEQUAL"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                            <texture name="pointLightShadowMap10" fromLibrary="Froxels" toUnit="12">
                                <COMPARE_MODE value="COMPARE_REF_TO_TEXTURE"/>
                                <COMPARE_FUNC value="LEQUAL"/>
                                <MIN_FILTER value="LINEAR"/>
                                <MAG_FILTER value="LINEAR"/>
                            </texture>
                        </textures>
                    </map>
                </injectionMaps>
                <materialMaps>
                    <map fromMaterial="skyBox" toLibrary="Froxels" toMaterial="skyBox"/>
                </materialMaps>
            </pass>
        </pipeline>
    </pipelines>

    <interface>
        <window label="settings">
            <var label="Ambient light" type="LIGHT" context="Sun" component="AMBIENT_LIGHT_STRENGTH" def="min=0 max=1"/>
            <var label="Volumetric effects" type="RENDERER" context="CURRENT" component="VOL_ACTIVE" strings="Active,Inactive"/>
        </window>
        <window label="Fog">
            <var label="Anisotropy" type="STATE" context="Froxels::calcScattering" component="ANISOTROPY" def="min=0 max=0.999"/>
            <var label="Scattering" type="STATE" context="Froxels::calcScattering" component="SCATTERING" def="min=0 max=1"/>
            <var label="Absorption" type="STATE" context="Froxels::calcScattering" component="ABSORPTION" def="min=0.001 max=1"/>
            <var label="Density" type="STATE" context="Froxels::calcScattering" component="DENSITY" def="min=0.001 max=0.1"/>
